<%= simple_form_for(run) do |f| %>
  <%= f.error_notification %>
  <%= f.input :title %>
  <%= f.input :distance %>
  <%= f.input :duration %>
  <%= f.input :started_at %>
  <%= f.input :ended_at %>
  <%= f.input :finished %>
  <%= f.input :paused %>
  <%= f.input :note %>
  <%= f.submit %>
<% end %>

